<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="awesomegroupkolond.kontraktor.Tidsplan">


    <TextView
        android:id="@+id/textView"
        android:layout_width="180dp"
        android:layout_height="38dp"
        android:text="Arbejdet påbegyndes den:"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="87dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="og afsluttes den:"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="157dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ved ændringer i arbejdet ændres tidsplanen i overensstemmelse hermed, jf. tillægsaftalen."
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="259dp" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="171dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="textMultiLine"
        tools:layout_editor_absoluteX="174dp"
        tools:layout_editor_absoluteY="69dp" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textMultiLine"
        tools:layout_editor_absoluteX="122dp"
        tools:layout_editor_absoluteY="138dp" />
</android.support.constraint.ConstraintLayout>
